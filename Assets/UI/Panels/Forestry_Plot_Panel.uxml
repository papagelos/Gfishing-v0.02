                <ui:Button name="Btn_Close" text="X" class="fp-close" style="width: 28px; height: 28px; font-size: 14px;" />
            </ui:VisualElement>
            <ui:VisualElement name="Body" class="fp-body" style="flex-direction: row;">
                <ui:VisualElement name="LeftColumn" class="fp-col" style="flex: 1; margin-right: 8px;">
