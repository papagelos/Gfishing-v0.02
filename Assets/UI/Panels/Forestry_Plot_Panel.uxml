<ui:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ui="UnityEngine.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement name="ForestryMinigameRoot" class="fp-overlay" style="position: absolute; left: 0; top: 0; right: 0; bottom: 0; display: none;">
        <ui:VisualElement name="ForestryMinigameBackdrop" class="fp-backdrop" style="position: absolute; left: 0; top: 0; right: 0; bottom: 0; background-color: rgba(0,0,0,0.6);" />
        <ui:VisualElement name="ForestryMinigamePanel" class="fp-panel" style="position: absolute; left: 50%; top: 50%; translate: -50% -50%; width: 600px; background-color: rgb(40, 45, 55); border-radius: 8px; padding: 16px;">
            <ui:VisualElement name="Header" class="fp-header" style="flex-direction: row; justify-content: space-between; margin-bottom: 16px;">
                <ui:VisualElement class="fp-header-left">
                    <ui:Label name="TitleLabel" text="FORESTRY STATION" style="font-size: 18px; color: rgb(255, 255, 255); -unity-font-style: bold;" />
