                                <ui:ProgressBar name="PlotProgress_0" low-value="0" high-value="100" value="0" style="flex: 1; height: 16px;" />
                                <ui:Label name="PlotStatus_0" text="0%" style="width: 40px; font-size: 10px; color: rgb(150, 150, 150); -unity-text-align: middle-right;" />
                            </ui:VisualElement>
                            <ui:VisualElement name="PlotRow_1" class="fp-plot-row" style="flex-direction: row; align-items: center; margin-bottom: 6px;">
                                <ui:Label name="PlotLabel_1" text="Plot 2" style="width: 50px; font-size: 11px; color: rgb(180, 180, 180);" />
                                <ui:ProgressBar name="PlotProgress_1" low-value="0" high-value="100" value="0" style="flex: 1; height: 16px;" />
                                <ui:Label name="PlotStatus_1" text="0%" style="width: 40px; font-size: 10px; color: rgb(150, 150, 150); -unity-text-align: middle-right;" />
                            </ui:VisualElement>
                            <ui:VisualElement name="PlotRow_2" class="fp-plot-row" style="flex-direction: row; align-items: center; margin-bottom: 6px;">
                                <ui:Label name="PlotLabel_2" text="Plot 3" style="width: 50px; font-size: 11px; color: rgb(180, 180, 180);" />
                                <ui:ProgressBar name="PlotProgress_2" low-value="0" high-value="100" value="0" style="flex: 1; height: 16px;" />
                                <ui:Label name="PlotStatus_2" text="0%" style="width: 40px; font-size: 10px; color: rgb(150, 150, 150); -unity-text-align: middle-right;" />
                            </ui:VisualElement>
                            <ui:VisualElement name="PlotRow_3" class="fp-plot-row" style="flex-direction: row; align-items: center; margin-bottom: 6px; display: none;">
                                <ui:Label name="PlotLabel_3" text="Plot 4" style="width: 50px; font-size: 11px; color: rgb(180, 180, 180);" />
                                <ui:ProgressBar name="PlotProgress_3" low-value="0" high-value="100" value="0" style="flex: 1; height: 16px;" />
                                <ui:Label name="PlotStatus_3" text="0%" style="width: 40px; font-size: 10px; color: rgb(150, 150, 150); -unity-text-align: middle-right;" />
                            </ui:VisualElement>
                            <ui:VisualElement name="PlotRow_4" class="fp-plot-row" style="flex-direction: row; align-items: center; margin-bottom: 6px; display: none;">
                                <ui:Label name="PlotLabel_4" text="Plot 5" style="width: 50px; font-size: 11px; color: rgb(180, 180, 180);" />
                                <ui:ProgressBar name="PlotProgress_4" low-value="0" high-value="100" value="0" style="flex: 1; height: 16px;" />
                                <ui:Label name="PlotStatus_4" text="0%" style="width: 40px; font-size: 10px; color: rgb(150, 150, 150); -unity-text-align: middle-right;" />
                            </ui:VisualElement>
                            <ui:VisualElement name="PlotRow_5" class="fp-plot-row" style="flex-direction: row; align-items: center; margin-bottom: 6px; display: none;">
                                <ui:Label name="PlotLabel_5" text="Plot 6" style="width: 50px; font-size: 11px; color: rgb(180, 180, 180);" />
                                <ui:ProgressBar name="PlotProgress_5" low-value="0" high-value="100" value="0" style="flex: 1; height: 16px;" />
                                <ui:Label name="PlotStatus_5" text="0%" style="width: 40px; font-size: 10px; color: rgb(150, 150, 150); -unity-text-align: middle-right;" />
                            </ui:VisualElement>
                        </ui:VisualElement>
                    </ui:VisualElement>
                    <ui:VisualElement class="fp-card" style="background-color: rgb(50, 55, 65); border-radius: 6px; padding: 12px;">
                        <ui:Label text="ACTIONS" style="font-size: 11px; color: rgb(150, 150, 150); margin-bottom: 8px;" />
                        <ui:VisualElement style="flex-direction: row; justify-content: space-between;">
                            <ui:Button name="Btn_HarvestAll" text="HARVEST READY" style="flex: 1; margin-right: 4px; height: 32px;" />
                            <ui:Button name="Btn_PlantAll" text="PLANT EMPTY" style="flex: 1; margin-left: 4px; height: 32px;" />
                        </ui:VisualElement>
                        <ui:Label name="ActionResultLabel" text="" style="font-size: 11px; color: rgb(100, 200, 100); margin-top: 8px; -unity-text-align: middle-center;" />
