                            <ui:Label name="GrowthSpeedLabel" text="100%" style="font-size: 24px; color: rgb(100, 220, 100); -unity-font-style: bold;" />
                            <ui:Label name="GrowthBonusLabel" text="+0% (0 forest tiles)" style="font-size: 11px; color: rgb(150, 200, 150);" />
                        </ui:VisualElement>
                        <ui:Label name="GrowthHintLabel" text="Adjacent forest tiles boost growth speed." style="font-size: 10px; color: rgb(120, 120, 120); margin-top: 4px;" />
                    </ui:VisualElement>
                    <ui:VisualElement class="fp-card" style="background-color: rgb(50, 55, 65); border-radius: 6px; padding: 12px; margin-bottom: 8px;">
                        <ui:Label text="OUTPUT PER HARVEST" style="font-size: 11px; color: rgb(150, 150, 150); margin-bottom: 4px;" />
                        <ui:VisualElement style="flex-direction: row; justify-content: space-between;">
                            <ui:Label name="WoodOutputLabel" text="Wood: 3" style="font-size: 14px; color: rgb(180, 140, 100);" />
                            <ui:Label name="FiberOutputLabel" text="Fiber: 1" style="font-size: 14px; color: rgb(150, 200, 150);" />
                        </ui:VisualElement>
                    </ui:VisualElement>
                    <ui:VisualElement class="fp-card" style="background-color: rgb(50, 55, 65); border-radius: 6px; padding: 12px;">
                        <ui:Label text="STATUS" style="font-size: 11px; color: rgb(150, 150, 150); margin-bottom: 4px;" />
                        <ui:Label name="StatusSummaryLabel" text="3/3 plots active, 0 ready" style="font-size: 12px; color: rgb(200, 200, 200);" />
                        <ui:Label name="PlotsUnlockedLabel" text="Plots: 3/6 unlocked" style="font-size: 11px; color: rgb(150, 150, 150); margin-top: 4px;" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement name="RightColumn" class="fp-col" style="flex: 1.5; margin-left: 8px;">
                    <ui:VisualElement class="fp-card" style="background-color: rgb(50, 55, 65); border-radius: 6px; padding: 12px; margin-bottom: 8px;">
                        <ui:Label text="FOREST PLOTS" style="font-size: 11px; color: rgb(150, 150, 150); margin-bottom: 8px;" />
                        <ui:VisualElement name="PlotsList" style="flex-direction: column;">
